{
  "name": "@laststance/react-next-eslint-plugin",
  "version": "2.0.0",
  "description": "ESLint plugin for React and Next.js projects with no-jsx-without-return rule",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "lint",
    "linter",
    "react",
    "nextjs",
    "jsx"
  ],
  "author": "laststance",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laststance/react-next-eslint-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/laststance/react-next-eslint-plugin/issues"
  },
  "homepage": "https://github.com/laststance/react-next-eslint-plugin#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "mocha tests/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "test:todo-e2e": "pnpm --filter todo-lint-app test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -p tests/types/tsconfig.json",
    "push-release-commit": "npx push-release-commit"
  },
  "peerDependencies": {
    "eslint": "^9.0.0 || ^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@laststance/npm-publish-tool": "^2.0.0",
    "eslint": "^10.0.2",
    "mocha": "^11.7.5",
    "prettier": "^3.8.1",
    "release-it": "^19.2.4",
    "typescript": "5.9.3"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "lib/",
    "docs/",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}